@article{ISI:A1991FD97000001,
  author = {YAMAMOTO, H and OKUYAMA, T and YOSHIDA, M and SUGIYAMA, K},
  title = {{GENERATION PROCESS OF GROWTH STRESSES IN CELL-WALLS .3. GROWTH STRESS IN COMPRESSION WOOD}},
  journal = {{MOKUZAI GAKKAISHI}},
  year = {{1991}},
  volume = {{37}},
  number = {{2}},
  pages = {{94-100}},
  abstract = {{To obtain an experimental foundation for a hypothesis on the origin of growth stress in the cell wall, the numerical relationships between anatomical factors, Klason lignin content, and the longitudinal released strain of the growth stress were investigated, especially in the compression wood region. In the compression wood region, the longitudinal expansive released strain had positive correlations with the Klason lignin content, the microfibril angle (MFA), the darkness of the color, and the fractions of the cell wall area and the intercellular space area. It had also an inverse correlation with Young's modulus in the cell wall substance. Longitudinal expansive released strains were measured also in the opposite wood region in spite of the small values. The following conclusions were made. (1) In the compression wood region, generation of the compressive growth stress is controlled by an interaction between a large MFA and a large deposition of lignin in addition to anatomical factors, the thickness of the cell wall, and the Young's modulus of the cell wall substance. This suggests that the mechanism of the lignin swelling hypothesis takes part in the generation of the compressive growth stress in the compression wood region. (2) The longitudinal expansive released strain becomes larger with the increase of the degrees of compression wood. Therefore, the degrees of compression wood can be estimated easily and accurately by measuring the longitudinal released strain of the growth stress.}},
  publisher = {{JAPAN WOOD RES SOC}},
  address = {{1-1-17, MUKOGAOKA, BUNKYO-KU, TOKYO, 113-0023, JAPAN}},
  type = {{Article}},
  language = {{English}},
