@article{ISI:A1993LM28800002,
  author = {YAMAMOTO, H and OKUYAMA, T and YOSHIDA, M},
  title = {{GENERATION PROCESS OF GROWTH STRESSES IN CELL-WALLS .5. MODEL OF TENSILE-STRESS GENERATION IN GELATINOUS FIBERS}},
  journal = {{MOKUZAI GAKKAISHI}},
  year = {{1993}},
  volume = {{39}},
  number = {{2}},
  pages = {{118-125}},
  abstract = {{To clarify the contribution of the gelatinous layer (G-layer) to the generation of large tensile growth stress, we simulated the experimental result in tension wood by using a mechanical model of G-fibers. The simulation revealed the following results. The cellulose microfibril generates large tensile stress during cell maturation. In consequence the G-fibers generate a large tensile stress in the longitudinal direction. This supports the cellulose tension hypothesis of the generation mechanism of the tensile growth stress in the tension wood which forms G-fibers.}},
